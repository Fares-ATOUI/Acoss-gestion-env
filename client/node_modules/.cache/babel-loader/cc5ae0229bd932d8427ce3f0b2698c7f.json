,aAZA,yBAYA,KAZA,EAYA;AACA,UAAA,QAAA,GAAA,KAAA,OAAA,CAAA,OAAA,CAAA,KAAA,CAAA;AACA,MAAA,QAAA,KAAA,CAAA,CAAA,GACA,KAAA,OAAA,CAAA,MAAA,CAAA,QAAA,EAAA,CAAA,CADA,GAEA,KAAA,OAAA,CAAA,IAAA,CAAA,KAAA,CAFA;AAGA;AAjBA;AA7BA,CAAA","sourcesContent":["<template>\n  <CDataTable\n    :items=\"items\"\n    :fields=\"fields\"\n    column-filter\n    table-filter\n    items-per-page-select\n    hover\n    sorter\n    pagination\n  >\n    <template #status=\"{item}\">\n      <td>\n        <CBadge :color=\"getBadge(item.status)\">{{item.status}}</CBadge>\n      </td>\n    </template>\n    <template #show_details=\"{item, index}\">\n      <td class=\"py-2\">\n        <CButton\n          color=\"primary\"\n          variant=\"outline\"\n          square\n          size=\"sm\"\n          @click=\"toggleDetails(index)\"\n        >{{details.includes(index) ? 'Masquer' : 'Afficher'}}</CButton>\n      </td>\n    </template>\n    <template #details=\"{item, index}\">\n      <CCollapse :show=\"details.includes(index)\">\n        <CCardBody>\n          <CMedia :aside-image-props=\"{ height: 102 }\">\n            <h4>{{item.code_projet}}</h4>\n            <p class=\"text-muted\">Date MEP: {{item.date_mep}}</p>\n            <CButton size=\"sm\" color=\"info\" class>User Settings</CButton>\n            <CButton size=\"sm\" color=\"danger\" class=\"ml-1\">Supprimer</CButton>\n          </CMedia>\n        </CCardBody>\n      </CCollapse>\n    </template>\n  </CDataTable>\n</template>\n\n<script>\nconst fields = [\n  { key: \"code_projet\", _style: \"width:40%\" },\n  { key: \"libelle\", _style: \"width:40%;\" },\n  { key: \"id_projet_lot\", _style: \"width:20%;\" },\n  {\n    key: \"show_details\",\n    label: \"\",\n    _style: \"width:1%\",\n    sorter: false,\n    filter: false\n  }\n];\n\nexport default {\n  props: [\"items\"],\n  \n\n  computed: {\n    getItems() {\n      return this.items;\n    }\n  },\n  name: \"tables\",\n  data() {\n    return {\n      fields,\n      details: []\n    };\n  },\n\n  /*\n  mounted: function() {\n    fetch(\"http://localhost:3000/projet_mm_date_mep\")\n      .then(rep => {\n        return rep.json();\n      })\n      .then(e => {\n        this.items = e;\n      });\n  },\n*/\n\n  methods: {\n    getBadge(status) {\n      return status === \"Active\"\n        ? \"success\"\n        : status === \"Inactive\"\n        ? \"secondary\"\n        : status === \"Pending\"\n        ? \"warning\"\n        : status === \"Banned\"\n        ? \"danger\"\n        : \"primary\";\n    },\n    toggleDetails(index) {\n      const position = this.details.indexOf(index);\n      position !== -1\n        ? this.details.splice(position, 1)\n        : this.details.push(index);\n    }\n  }\n};\n</script>"],"sourceRoot":"src/components"}]}